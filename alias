# shortcuts for terminal #

# school programs #
alias lc3edit="wine ~/Desktop/LC3/LC3Edit/LC3Edit.exe"
alias lc3sim="wine ~/Desktop/LC3/Simulate.exe"

# easy update from bash to alias file #
updateAlias(){
	tail -$1 ~/.bash_profile >> ~/Desktop/OwnDev/alias
}
alias upa=updateAlias

# alias for python editor 
pythonEditor(){
    autopep8 --in-place --aggressive $1
}
alias autoFormatPython=pythonEditor

# alias for sshing to school
alias calpoly="ssh kaabdull@unix2.csc.calpoly.edu"
